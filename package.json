{
  "name": "telegram-audio-pack",
  "version": "1.1.3",
  "private": true,
  "description": "Telegram audio pack, like sticker pack",
  "author": "Alaladdin",
  "keywords": [
    "nestjs",
    "typesript",
    "telegraf",
    "mongodb",
    "typegoose"
  ],
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/schedule": "^2.2.1",
    "@ntegral/nestjs-sentry": "^4.0.0",
    "@sentry/node": "^7.51.2",
    "@typegoose/typegoose": "^11.1.0",
    "adm-zip": "^0.5.10",
    "axios": "^1.4.0",
    "cache-manager": "^5.2.1",
    "cache-manager-redis-store": "^3.0.1",
    "class-validator": "^0.14.0",
    "fluent-ffmpeg": "^2.1.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^7.1.1",
    "mongoose-lean-virtuals": "^0.9.1",
    "ms": "^3.0.0-beta.2",
    "nestjs-i18n": "10.2.6",
    "nestjs-telegraf": "^2.6.3",
    "nestjs-typegoose": "^7.1.38",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "telegraf": "^4.12.2",
    "tmp-promise": "^3.0.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.1.0",
    "@sentry/types": "^7.51.2",
    "@types/adm-zip": "^0.5.0",
    "@types/cache-manager": "^4.0.2",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/cron": "^2.0.1",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/lodash": "^4.14.194",
    "@types/node": "20.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typegram": "^5.0.0",
    "typescript": "4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
